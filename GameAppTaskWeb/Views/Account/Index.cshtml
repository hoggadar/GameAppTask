@model IList<GameAppTaskDataAccess.Models.UserModel>

@{
    ViewData["Title"] = "Users";
}

<h2>@ViewData["Title"]</h2>

<table>
    <thead>
        <tr>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
            </tr>
        }
    </tbody>
</table>